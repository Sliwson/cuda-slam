{
  "$schema": "http://json-schema.org/draft-04/schema",
  "type": "object",
  "required": [ "before-path", "after-path", "method" ],
  "properties": {
    "before-path": {
      "type": "string"
    },
    "after-path": {
      "type": "string"
    },
    "method": {
      "type": "string",
      "enum": [ "icp", "nicp", "cpd" ]
    },
    "policy": {
      "type": "string",
      "enum": [ "sequential", "parallel" ]
    },
    "scale": {
      "type": "number"
    },
    "translation": {
      "type": "array",
      "minLength": 3,
      "maxLength": 3,
      "items": {
        "type": "number"
      }
    },
    "rotation": {
      "type": "array",
      "minLength": 9,
      "maxLength": 9,
      "items": {
        "type": "number"
      }
    },
    "translation-range": {
      "type": "number"
    },
    "angle-range": {
      "type": "number"
    },
    "max-iterations": {
      "type": "integer"
    },
    "cloud-before-resize": {
      "type": "integer"
    },
    "cloud-after-resize": {
      "type": "integer"
    },
    "cloud-spread": {
      "type": "number"
    },
    "random-seed": {
      "type": "integer"
    },
    "show-visualisation": {
      "type": "boolean"
    },
    "max-distance-squared": {
      "type": "number"
    },
    "approximation-type": {
      "type": "string",
      "enum": [ "none", "hybrid", "full" ]
    },
    "nicp-batch-size": {
      "type": "number"
    },
    "nicp-iterations": {
      "type": "number"
    },
    "nicp-subcloud-size": {
      "type": "number"
    },
    "cpd-weight": {
      "type": "number"
    },
    "cpd-const-scale": {
      "type": "boolean"
    },
    "cpd-tolerance": {
      "type": "number"
    },
    "convergence-epsilon": {
      "type": "number"
    },
    "noise-affected-points-before": {
      "type": "number"
    },
    "noise-affected-points-after": {
      "type": "number"
    },
    "noise-intensity-before": {
      "type": "number"
    },
    "noise-intensity-after": {
      "type": "number"
    },
    "additional-outliers-before": {
      "type": "integer"
    },
    "additional-outliers-after": {
      "type": "integer"
    }
  }
}
